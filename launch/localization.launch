<launch>

    <include file="$(find elikos_localization)/launch/localization_detection.launch">
        <arg name="camera_name" value="gazebo_ffmv_bottom"/>
    </include>
    <include file="$(find elikos_localization)/launch/localization_detection.launch">
        <arg name="camera_name" value="gazebo_r200_front"/>
    </include>

    <node pkg="elikos_localization" type="fallback.py" name="feature_tracking" output="screen">
        <rosparam command="load" file="$(find elikos_localization)/cfg/feature_tracking.yaml"/>
        <remap from="localization/features_0" to="gazebo_ffmv_bottom_intersections"/>
        <remap from="localization/features_1" to="gazebo_r200_front_intersections"/>
        <remap from="inverse_transform" to="elikos_ffmv_bottom_camera/image_preprocessed/inverse_transform"/>
    </node>
</launch>
